{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [ "dom", "es5", "scripthost", "es2015.promise", "es2015" ],
    "target": "es5",
    "sourceMap": true,
    "strictNullChecks": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
